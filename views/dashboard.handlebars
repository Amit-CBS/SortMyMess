        {{success_msg}}
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    {{/if}}

    <center>
        <button id="myBtn" onclick="editmodal()" class="btn btn3">Add</button>
        <button id="myBtn1" onclick="showmodal()" class="btn btn3">Show</button>
        <button id="myBtn2" onclick="calculatemodal()" class="btn btn3">Calculte</button>
    </center>
    <div class="row justify-content-sm-center">
        <div class="col-sm-8 thumbnail">
            <table class="table">
                <thead>
                    <tr>
                        <th>MessId</th>
                        <th>Name</th>
                        <th>Email</th>
                        <th>Phone</th> 
                    </tr>
                </thead>
                <tbody>
                    {{#each memberUser}}
                        <tr>
                            <td>{{messid1}}</td>
                            <td>{{name}}</td>
                            <td>{{email}}</td>
                            <td>{{phone}}</td>
                            <td>{{member}}</td>
                        </tr>
                    {{/each}}
                </tbody>
            </table>
        </div>
    </div>

    
                        {{#each dataUser}}
                        
                            <div class="nameId" style="display:none">{{nameId}}</div>
                            <div class="item" style="display:none">{{item}}</div>
                            <div class="price" style="display:none">{{price}}</div>
                        
                        {{/each}}
                        <div id="data"></div>
                        
                   
    <div id="calculate" class="modal thumbnail">
        <div class="modal-content2">
            <span class="close3">&times;</span>
            <div class="col-sm-8">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Total Expense</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each memberTotal}}
                        <tr>
                            <td>{{name}}</td>
                            <td>{{values}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
