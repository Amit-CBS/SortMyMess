<div class="container" style="color: white;">
    {{#if success_msg}}
    <div class="alert alert-success alert-dismissible fade show" role="alert">
        {{success_msg}}
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
    </div>
    {{/if}}
    <button class="btn btn2" id="myBtn" onclick="editmodal()">Add</button>
    <button class="btn btn1" id="myBtn1" onclick="showmodal()">Show</button>
    <button class="btn btn1" id="myBtn2" onclick="calculatemodal()">Calculte</button>
        
    <div class="col-sm-8">
        <table class="table">
            <thead>
                <tr>
                    <th>MessId</th>
                    <th>Name</th>
                    <th>Email</th>
                    <th>Phone</th> 
                </tr>
            </thead>
            <tbody>
                {{#each memberUser}}
                    <tr>
                        <td>{{messid1}}</td>
                        <td>{{name}}</td>
                        <td>{{email}}</td>
                        <td>{{phone}}</td>
                        <td>{{member}}</td>
                    </tr>
                {{/each}}
            </tbody>
        </table>
    </div>


    <div id="show" class="modal">
        <div class="modal-content">
            <span class="close">&times;</span>
            <div class="col-sm-8">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Items</th>
                            <th>Price</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each dataUser}}
                        <tr>
                            <td>{{nameId}}</td>
                            <td>{{item}}</td>
                            <td>{{price}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>


    <div id="calculate" class="modal">
        <div class="modal-content2">
            <span class="close3">&times;</span>
            <div class="col-sm-8">
                <table class="table">
                    <thead>
                        <tr>
                            <th>Name</th>
                            <th>Total Expense</th>
                        </tr>
                    </thead>
                    <tbody>
                        {{#each memberTotal}}
                        <tr>
                            <td>{{name}}</td>
                            <td>{{values}}</td>
                        </tr>
                        {{/each}}
                    </tbody>
                </table>
            </div>
        </div>
    </div>


    <div id="edit" class="modal">
        <div class="modal-content">
            <span class="close2">&times;</span>
            <form action="/edit" method="POST" class="box">
                <center>
                    <div class="form-group thumbnail">
                        <label for="Item Name">Item Name</label>
                        <input required type="text" class="form-control" placeholder="Item Name" name="itemName">
                        <input required type="hidden" name="idUser" value="{{email}}">
                    </div>
                    <div class="form-group thumbnail">
                        <label for="Price">Price</label>
                        <input required type="number" class="form-control"  placeholder="Price" name="itemPrice">
                    </div>
                    <button type="submit" class="btn btn-primary">Submit</button>
                </center>
            </form>
        </div>
    </div>
</div>